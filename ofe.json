{
  "default": {
    "extensions": [".py", ".md"],
    "exclusions": ["node_modules/"],
    "files": ["README.md"],
    "first_file": "README.md"
  },
  "docs": {
    "extensions": [".md"],
    "first_file": "README.md"
  },
  "diff": {
    "command": "git diff --name-only"
  },
  "git": {
    "files": [".git/config", ".gitignore"]
  }
}
